# kate: icon-border true; indent-mode none; syntax INI Files;

[run]
source = schvalidator
branch = True

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if 0:
    if False:
    if __name__ == .__main__.:

[html]
title = Coverage Report
